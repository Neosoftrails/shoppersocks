$j(document).ready(function(){
	
		//For Modal Popup
	//Show products related to category
	//Function called on click of category
	//FUNCTION 1
	$j(".categoryId").click(function(){
		$j("#rightbootimage").html('');
		Element.show('loadingmask');
		var cate = $j(this).attr('id');
		
		var $j_object = $j(".socksdisplay");
		$j_object.each( function(i) { 
			$j(this).hide();
		});
		var bootColor = $j("#bootcolor").val();
	//setting new link to show all list
		$j("#socks"+cate).show('slow');
		var productid = "cid="+cate+"&bootc="+bootColor;
		$j.post("http://cardysocks.co.uk/index.php/productlist/index/ajax/", productid,
	    function(data){
	    	$j('.blacktital_1').html(data.m1);
	    	$j("#rightbootimage").html(data.m11);
	    	Element.hide('loadingmask');
	    	if(cate == 3){
	    	$j("#sockcheck").val('2');
	    	}
	    	else{
	    	$j("#sockcheck").val('4');	
	    	}
	        },'json');
		  
	  //Show title of select sock color and link toshow all product list	
	  	$j('.blacktital').show('slow');
		$j('.blacktital_1').show('slow');
		
	});
	//On select of image change See all div link
	//Change Image on right on click of image
	//Function called on click of product
	//FUNCTION 2
	$j(".selproduct").click(function(){
		$j("#rightbootimage").html('');
		Element.show('loadingmask');
		var prodcat = $j(this).attr('id').split("::");
		var bootColor = $j("#bootcolor").val();
		var productid = "pcid="+prodcat[0]+"&cid="+prodcat[1]+"&bootc="+bootColor;
		$j('#currcatprodid').val(prodcat[0]);
		$j.post("http://cardysocks.co.uk/index.php/productlist/index/ajax/", productid,
        function(data){
        	$j('.blacktital_1').html(data.m1);
        	$j('#rightbootimage').html(data.m2);
        	$j('#rightbootimagename').html(data.m99);
          $j('#productprice').html(data.price);
          $j('#productbasket').show();
        	$j('#placesockinbasket').html(data.m3);
        	$j('#Emailfriend').html(data.m4); 
			$j('#twitter').html(data.m5); 
			$j('#facebook').html(data.m6); 
			$j('#blacks').attr('alt',data.m10);
			$j('#browns').attr('alt',data.m9);
			$j('#creams').attr('alt',data.m8);
			$j('#whites').attr('alt',data.m7);
			Element.hide('loadingmask');
			$j("#sockcheck").val('3');
        },'json');    	
	});

